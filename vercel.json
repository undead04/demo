{
  "version": 2,
  "builds": [
    {
      "src": "new-express-project/api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "my-app/src/package.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "new-express-project/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "my-app/src/index.js"
    }
  ]
}
